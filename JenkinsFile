pipeline {
     stages{
       stage ('compile stage'){
              steps{
              withmaven(maven : 'maven_3_6_3') {
                   sh 'mvn clean compile'
       }
       }
       }
       stage ('testing stage'){
              steps{
              withmaven(maven : 'maven_3_6_3') {
                   sh 'mvn test'
      }
      }
      }
       stage ('deployment stage'){
              steps{
              withmaven(maven : 'maven_3_6_3') {
                   sh 'mvn deploy'
      }
      }
      }
}
}
